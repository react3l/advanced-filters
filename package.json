{
  "name": "react3l-core",
  "version": "0.1.0-beta",
  "description": "React3L Core",
  "author": "thanhtunguet",
  "license": "MIT",
  "repository": "thanhtunguet/react3l-core",
  "main": "./index.js",
  "module": "./index.js",
  "jsnext:main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
